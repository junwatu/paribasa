#!/usr/bin/env node
'use strict'

const paribasan = require('../index.js')

if (process.argv[2] === 'search') {
  paribasan.search(process.argv[3]).then((searchResult) => {
    console.log(searchResult)
  })
} else if (process.argv[2] === 'init') {
  paribasan.indexing().then((status) => {
    console.log('Indexing ok')
  })
} else {
  console.log(paribasan.get())
}
